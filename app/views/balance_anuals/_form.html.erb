<%= simple_form_for(@balance_anual) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :partido %>
    <%= f.input :fecha_datos %>
    <%= f.input :concepto %>
    <%= f.input :tipo %>
    <%= f.input :importe %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
