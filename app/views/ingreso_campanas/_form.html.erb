<%= simple_form_for(@ingreso_campana) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :partido %>
    <%= f.input :fecha_datos %>
    <%= f.input :fecha_eleccion %>
    <%= f.input :rut_candidato %>
    <%= f.input :rut_donante %>
    <%= f.input :nombre_donante %>
    <%= f.input :fecha_documento %>
    <%= f.input :tipo_documento %>
    <%= f.input :numero_documento %>
    <%= f.input :numero_cuenta %>
    <%= f.input :glosa %>
    <%= f.input :monto %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
