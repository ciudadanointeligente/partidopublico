<div class="container">
  <h2 class="page-header">Trámites</h2>

  <div class="row">
    <%= render :layout => 'form' do |f| %>
      <%= f.fields_for :tramites do |ff| %>
        <%= render 'tramite_fields', :f => ff %>
      <% end %>

      <div class="col-md-12">
        <%= link_to_add_association f, :tramites, class: 'btn btn-default' do %>
          <i class="fa fa-plus"></i> Agregar trámite
        <% end %>
      </div>
    <% end %>
  </div>
</div>