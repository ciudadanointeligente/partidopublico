<script>
$(document).ready(function(){
  var presencia = <%= raw @datos_total_nacional.to_json %>,
      colors = ["#23dbb8", "#19a58a", "#245d72", "#1ed6d6", "#17a5a5"];

  $.each(presencia, function(k,v){
    if( v.ordinal != '') {
      el = $( "#region-mapa-" + v.ordinal );
      el.addClass('presencia');
    }
  })

  $('path.presencia').on('click', function(){
    $('path').removeClass('active');
    $(this).addClass('active');

    tmp = $(this)[0].id.split("-")
    el = $("#"+tmp[0]+"-"+tmp[2])

    $('#select-region').val(tmp[0]+"-"+tmp[2])

    $('.showData').addClass('hideData').removeClass('showData');
    el.removeClass('hideData').addClass('showData')
  })

  $('#select-region').on('change', function(){
    selected_opt = $(this)[0].value;
    tmp_path = selected_opt.split('-');
    el_path = tmp_path[0]+'-mapa-'+tmp_path[1];

    $('path').removeClass('active');
    $("#"+el_path).addClass('active');

    $('.showData').addClass('hideData').removeClass('showData');
    $('#'+selected_opt).removeClass('hideData').addClass('showData')
  })

  for( var x = 0, y = presencia.length; x<y; x++ ){
    if(presencia[x].hombres > 0 && presencia[x].mujeres > 0 ) {
      el_select = document.getElementById('select-region')
      el_select.options[el_select.options.length] = new Option(presencia[x].region, 'region-'+presencia[x].ordinal);

      $('#region-'+presencia[x].ordinal).find('h4').html( "Total de afiliados " + presencia[x].region )
      $('#region-'+presencia[x].ordinal).find('h6').html(presencia[x].hombres + presencia[x].mujeres)
      Raphael("circulo-"+presencia[x].ordinal, 170, 170).donutChart(85, 85, 70, 35, [presencia[x].hombres, presencia[x].mujeres], ['Hombres', 'Mujeres'], colors, true, $('#region-'+presencia[x].ordinal+' .leyenda'), colors);
      var data_desgloce = [],
          display_desgloce = 0;
      for( var v = 0, w = presencia[x].desgloce.length; v<w; v++ ) {
        for( key in presencia[x].desgloce[v] ){
         data_desgloce.push(presencia[x].desgloce[v][key])
         display_desgloce = display_desgloce + presencia[x].desgloce[v][key];
        }
      }

      cargo_colors = ["#99F4F0", "#1ED6D6", "#06B9BF", "#CDFEFC"];
      data_cargos = [presencia[x].cargos[0].nro_cargos, presencia[x].cargos[1].nro_cargos, presencia[x].cargos[2].nro_cargos, presencia[x].total]
      label_cargos = ["Cargos de dirección interna","Representantes electos","Autoridades designadas","Afiliados sin cargos"]
      if(data_cargos.length > 0)
        Raphael("cargos-"+presencia[x].ordinal, 170, 170).donutChart(85, 85, 70, 35, data_cargos, label_cargos, cargo_colors, true, $('#region-'+presencia[x].ordinal+' .leyenda-cargos'), cargo_colors);

      if(display_desgloce) {
        colors_range = ["#FDD7D2","#FFBCAA","#F28269","#E85D34","#B74B2B","#9A553A","#2C1313","#5E3F3F","#756666","#C5C0C0","#E9DEDF","#F4F2F2"];
        var leyenda = ["14 - 17 años","18 - 24 años","25 - 29 años","30 - 34 años","35 - 39 años","40 - 44 años","45 - 49 años","50 - 54 años","55 - 59 años","60 - 64 años","65 - 69 años","> 70 años"];
        Raphael("desgloce-"+presencia[x].ordinal, 170, 170).donutChart(85, 85, 70, 35, data_desgloce, leyenda, colors_range, true, $('#region-'+presencia[x].ordinal+' .leyenda-desgloce'), colors_range);
      }
    }
  }
});
</script>
