<% @fechas_datos.each do |f| %>
  <ul>
    <li><%= f %></li>

  </ul>
<% end %>

<select
    ng-change="selectAction()"
    ng-model="myOption"
    ng-options="value.id as value.label group by value.group for value in myOptions">
    <option>--</option>

</select>

<div id="barras"></div>

<script>
$(document).ready(function(){
  var data = <%=  @datos_ingresos_ordinarios.to_json.html_safe %>;
  var data_totals = <%=  @datos_ingresos_ordinarios_totals.to_json.html_safe %>;
  var fechas = <%=  @fechas_datos.to_json.html_safe %>;
  if (fechas.length > 0){
    Raphael("barras",700, 500 ).ingresos_ordinarios_chart(data, data_totals);
  } else {
    Raphael("barras",700, 200 ).no_data_chart()
  }

});
</script>
